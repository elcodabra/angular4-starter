{
  "name": "angular4-starter",
  "version": "1.0.0",
  "description": "Fullstack Starter Project with Angular 4 & Node JS",
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --config config/webpack.local.js --inline --progress --port=8080",
    "start:ng": "ng serve --environment local --proxy-config proxy.conf.json --host 0.0.0.0 --port 8080",
    "start:server": "nodemon ./server.js",
    "build": "npm run build:prod",
    "build:local": "npm run clean:dist && webpack --config config/webpack.local.js --env.dev",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --env.dev",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js --env.prod",
    "clean:dist": "rimraf -- dist",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sani-banani/angular4-starter.git"
  },
  "author": "a.konovalov88@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.3.6",
    "@angular/router": "^4.3.6",
    "compression": "^1.7.0",
    "core-js": "^2.5.0",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.3.6",
    "@angular/language-service": "^4.3.6",
    "@types/jasmine": "~2.5.54",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.25",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-loader": "^0.5.7",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "~3.5.5",
    "webpack-dev-server": "~2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  }
}
